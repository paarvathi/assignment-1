public class Main {
  public static void main(String[] args) {
    int a_int = 5;               
    float b_float = 5.99f;    
    char c_Letter = 'D';         
    boolean d_Bool = true;       
    String e_Text = "Hello";       
    System.out.println(a_int);
    System.out.println(b_Float);
    System.out.println(c_Letter);
    System.out.println(d_Bool);
    System.out.println(e_Text);
  }
}
