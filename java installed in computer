public class Main {
public static void main(String[] args) {
System.out.println("\nVersion: "+System.getProperty("java.version"));
System.out.println("time:"+System.getProperty("java.runtime.version"));
System.out.println("Java Home: "+System.getProperty("java.home"));
System.out.println("Java Vendor: "+System.getProperty("java.vendor"));
System.out.println("Java URL: "+System.getProperty("java.vendor.url"));
System.out.println("Path: "+System.getProperty("java.class.path")+"\n");
 }
}
